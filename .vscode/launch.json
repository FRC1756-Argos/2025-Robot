  "version": "0.2.0",
  "configurations": [
    {
      "type": "wpilib",
      "name": "WPILib Desktop Debug",
      "request": "launch",
      "desktop": true,
    },
    {
      "type": "wpilib",
      "name": "WPILib roboRIO Debug",
      "request": "launch",
      "desktop": false,
    },
    {
      "type": "debugpy",
      "name": "fieldCal",
      "program": "${workspaceFolder}/Tools/FieldCal/FieldCal.py",
      "request": "launch",
      "cwd": "${workspaceFolder}/Tools/FieldCal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/.venv/Lib/site-packages"
      },
      "console": "integratedTerminal"
    }
    // {
    //   "type": "debugpy",
    //   "name": "Python: Current File",
    //   "request": "launch",
    //   "program": "${file}",
    //   "console": "integratedTerminal",
    //   "env": {
    //     "PYTHONPATH": "${workspaceFolder}/.venv/Lib/site-packages;${env:PYTHONPATH}"
    //   }
    // }
  ]
}
